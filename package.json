{
  "name": "qwik-electron-app",
  "description": "Demo App with Routing built-in (recommended)",
  "main": "./dist/main/index.js",
  "version": "1.0.0",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/static/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "start": "vite --open --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "qwik": "qwik",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "electron:start": "yarn build && electron-vite preview",
    "electron:dev": "electron-vite dev --mode ssr",
    "electron:build": "VITE_ELECTRONBUILD=ELECTRONBUILD yarn build && yarn typecheck:node && electron-vite build",
    "electron:package": "electron-forge package",
    "electron:make": "electron-forge make",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "cap": "cap",
    "cap:sync": "cap sync",
    "cap:open:android": "cap open android",
    "cap:run:android": "cap run android",
    "cap:add:android": "cap add android"
  },
  "dependencies": {
    "@capacitor/android": "^5.0.4",
    "@capacitor/core": "^5.0.4",
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^1.0.2",
    "electron-serve": "^1.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^5.3.0"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.1.4",
    "@builder.io/qwik-city": "^1.1.4",
    "@capacitor/cli": "^5.0.4",
    "@electron-forge/cli": "^6.1.1",
    "@electron-forge/maker-deb": "^6.1.1",
    "@electron-forge/maker-rpm": "^6.1.1",
    "@electron-forge/maker-squirrel": "^6.1.1",
    "@electron-forge/maker-zip": "^6.1.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
    "@types/eslint": "^8.40.0",
    "@types/node": "^20.1.4",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "autoprefixer": "^10.4.14",
    "electron": "^25.0.1",
    "electron-is-dev": "^2.0.0",
    "electron-vite": "^1.0.23",
    "eslint": "^8.42.0",
    "eslint-plugin-qwik": "^1.1.4",
    "postcss": "^8.4.23",
    "prettier": "2.8.8",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.1.3",
    "undici": "5.22.1",
    "vite": "^4.3.9",
    "vite-tsconfig-paths": "4.2.0"
  }
}
